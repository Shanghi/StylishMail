               </Anchor>
            </Anchors>
         </Button>
         <Button name="StylishMailPreviewLoadButton" inherits="UIPanelButtonTemplate" text="Load">
            <Size>
               <AbsDimension x="82" y="22"/>
            </Size>
            <Anchors>
               <Anchor point="RIGHT" relativeTo="StylishMailPreviewSaveButton" relativePoint="LEFT">
                  <Offset>
                     <AbsDimension x="0" y="0"/>
                  </Offset>
               </Anchor>
            </Anchors>
