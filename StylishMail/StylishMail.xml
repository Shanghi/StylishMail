                  </Offset>
               </Anchor>
            </Anchors>
         </Button>
         <Frame name="StylishMailPreviewDropDown" inherits="UIDropDownMenuTemplate">
            <Anchors>
               <Anchor point="LEFT" relativeTo="StylishMailPreviewSender" relativePoint="RIGHT">
                  <Offset>
                     <AbsDimension x="-35" y="0"/>
                  </Offset>
               </Anchor>
            </Anchors>
         </Frame>       
      </Frames>
      <Scripts>
         <OnLoad>
            this:RegisterForDrag("LeftButton")
            this:RegisterEvent("MAIL_CLOSED")
         </OnLoad>
         <OnEvent>
            this:Hide()
         </OnEvent>
         <OnDragStart>
            this:StartMoving()
         </OnDragStart>
         <OnDragStop>
            this:StopMovingOrSizing()
         </OnDragStop>
         <OnMouseWheel>
            return
         </OnMouseWheel>
      </Scripts>
   </Frame>
</Ui>
